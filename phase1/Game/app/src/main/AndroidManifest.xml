<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.game">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OldAccountActivity"></activity>
        <activity android:name=".NewAccountActivity" />
        <activity
            android:name=".ui.login.NewAccountActivity"
            android:label="@string/title_activity_new_account"
            android:parentActivityName=".StartActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.game.StartActivity" />
        </activity>
        <activity android:name=".StartActivity" />
        <activity android:name=".level1.activities.BlackjackBetActivity" />
        <activity android:name=".level1.activities.EndGameActivity" />
        <activity android:name=".level1.activities.BlackjackPlayActivity" />
        <activity android:name=".level1.activities.BlackJackStartActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".level2.GameStartActivity" />
    </application>

</manifest>